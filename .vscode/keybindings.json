[
  {
    "key": "\\ backspace",
    "command": "rerunSearchEditorSearch",
    "when": "inSearchEditor"
  },
  {
    "key": "shift+cmd+r",
    "command": "-rerunSearchEditorSearch",
    "when": "inSearchEditor"
  },
  {
    "key": "shift+cmd+r",
    "command": "workbench.action.tasks.test"
  },
  {
    "key": "ctrl+alt+c",
    "command": "workbench.action.tasks.build"
  },
  {
    "key": "ctrl+alt+r",
    "command": "workbench.action.tasks.test"
  }
]
